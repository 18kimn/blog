{
		"name": "nathan-kim",
		"version": "1.0.0",
		"private": true,
		"description": "personal website for nathan kim",
		"author": "Nathan Kim",
		"type": "module",
		"scripts": {
				"dev": "svelte-kit dev",
				"build": "svelte-kit build",
				"package": "svelte-kit package",
				"preview": "svelte-kit preview",
