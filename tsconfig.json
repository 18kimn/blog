{
  "extends": "./server/build/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "noImplicitAny": false
  }
}
